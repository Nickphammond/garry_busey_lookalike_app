<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      
      <% if event.listed == true %>
        <tr>
          <td><%= event.date %></td>
          <td><%= event.time %></td>
          <td><%= link_to 'Show', event %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

<br>

