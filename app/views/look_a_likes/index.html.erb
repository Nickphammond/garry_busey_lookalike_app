<p id="notice"><%= notice %></p>

<h1>Look A Likes</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @look_a_likes.each do |look_a_like| %>
      <tr>
        <td><%= look_a_like.user.first_name %></td>
        <td><%= look_a_like.user.last_name %></td>
        <td><%= link_to 'Show', look_a_like %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Look A Like', new_look_a_like_path %>
